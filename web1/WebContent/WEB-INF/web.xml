		<servlet-name>/session2</servlet-name>
		<url-pattern>/session2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>/session3</servlet-name>
		<servlet-class>web1.SessionServlet3</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>/session3</servlet-name>
		<url-pattern>/session3</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>/dispatch</servlet-name>
		<servlet-class>web1.DispatchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>/dispatch</servlet-name>
		<url-pattern>/dispatch</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>/useBean</servlet-name>
		<servlet-class>web1.UseBeanServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>/useBean</servlet-name>
		<url-pattern>/useBean</url-pattern>
	</servlet-mapping>






</web-app>